<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.eventportal.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateEventServlet</servlet-name>
        <servlet-class>com.eventportal.servlet.CreateEventServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>com.eventportal.servlet.RegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterEventServlet</servlet-name>
        <servlet-class>com.eventportal.servlet.RegisterEventServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewParticipantsServlet</servlet-name>
        <servlet-class>com.eventportal.servlet.ViewParticipantsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FetchCalendarEventsServlet</servlet-name>
        <servlet-class>com.eventportal.servlet.FetchCalendarEventsServlet</servlet-class>
    </servlet>
