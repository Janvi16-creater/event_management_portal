        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>com.eventportal.servlet.LogoutServlet</servlet-class>
    </servlet>-->
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateEventServlet</servlet-name>
        <url-pattern>/CreateEventServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/RegisterServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterEventServlet</servlet-name>
        <url-pattern>/RegisterEventServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewParticipantsServlet</servlet-name>
        <url-pattern>/ViewParticipantsServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FetchCalendarEventsServlet</servlet-name>
        <url-pattern>/FetchCalendarEventsServlet</url-pattern>
    </servlet-mapping>
